<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Portfolio</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/
.styles.css" asp-append-version="true" />
  
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark navbar-bg-dark fixed-top">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index" >
                    Abdi Ali
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <div class="navbar-nav w-100 justify-content-end">
                        <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        <a class="nav-link " asp-area="" asp-controller="About" asp-action="About">About Me</a>
                        <a class="nav-link " asp-area="" asp-controller="Projects" asp-action="Projects">Projects</a>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <div>
        <main role="main" class="pb-3">
          

            @RenderBody()
        </main>
    </div>
    <br />
    <br />
    
    <footer class=" footertext-muted  text-White" style="padding-bottom: 10px;">
        <div class="container">
            <p>&copy; 2022 Abdi Ali. All rights reserved</p>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js//Default/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
  